<div class="hero">
    <div class="row justify-content-center">
        <div class="col-lg-3">
            <div class="container align-top">


                <div class="intro-wrap">
                    <h2 class="mb-5 text-white font-italic text-center"><span
                            class="d-block">Find your dream holidays!</span> Travel to <span
                            class="typed-words"></span></h2>

                    <div class="row mx-auto">
                        <div class="col-lg-12">
                            <form class="form">


                                <div class="row mb-2">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <input type="text" class="form-control text-center"
                                               placeholder="Your location..." data-toggle="tooltip"
                                               title="Your location..." value="Polska">
                                    </div>
                                </div>


                                <div class="row mb-2">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <input type="text" class="form-control text-center"
                                               placeholder="Your location..." data-toggle="tooltip"
                                               title="Your location..." value="Polska">
                                    </div>
                                </div>


                                <div class="row mb-2">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <input type="text" class="form-control text-center"
                                               placeholder="Where are you going?" data-toggle="tooltip"
                                               title="Where are you going?">
                                    </div>
                                </div>


                                <div class="row mb-2">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <input type="text" class="form-control text-center" name="daterange"
                                               data-toggle="tooltip"
                                               title="Range date start">
                                    </div>
                                </div>


                                <div class="row mb-4">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <div class="dropdown">
                                            <button class="form-control" type="button" id="dropdownMenuButton"
                                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <span id="numberOfAdults"></span> adults * <span
                                                    id="numberOfChldren"></span> children
                                            </button>


                                            <div class="dropdown-menu mx-2 col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-11"
                                                 aria-labelledby="dropdownMenuButton">
                                                <div class="row mb-2">
                                                    <div class="col-lg-5 text-center">
                                                        <b>Adults</b>
                                                    </div>
                                                    <div class="col-lg-5">
                                                        <input type="number" class="text-center" min="0" max="50"
                                                               step="1" value="2"
                                                               name="adults" id="adults">
                                                    </div>
                                                </div>
                                                <div class="row mb-2">
                                                    <div class="col-lg-5 text-center">
                                                        <b>Children</b>
                                                    </div>
                                                    <div class="col-lg-5">
                                                        <input type="number" class="text-center" min="0" max="50"
                                                               step="1" value="0"
                                                               name="children" id="children">
                                                    </div>
                                                </div>
                                                <div class="row mb-2 px-5">
                                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                                        <button type="button"
                                                                class="btn btn-primary btn-close btn-block"
                                                                onclick="getNumberPeople()">Select
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-12 col-md-6 mb-3 mb-lg-0 col-lg-12">
                                        <input type="submit" class="btn btn-primary btn-block" value="Search">
                                    </div>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
